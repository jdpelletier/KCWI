#!/bin/csh -f

echo "-- server startup debug levels --"
echo "0: (-v 0) No logging"
echo "1: (-v 256) Minimal logging (USER1 only)"
echo "2: (-v 270) Normal lab running (DEBUG 2-4, USER 1)"
echo "3: (-v 3840) Normal debugging, just application messages (USER 1-4)"
echo "4: (-v 3854) Normal debugging (DEBUG 2-4, USER 1-4)"
echo "5: (-v 3899) Normal debugging with portio (DEBUG 2-5+8, USER 1-4)"
echo "6: (-v 4095) Full on logging"
echo 
echo "-- other common levels --"
echo "2304 (USER1,4)"
echo "3844 (USER1-4 + DEBUG 3)"
echo "3972 (USER1-4 + DEBUG 3,8)"
echo
echo "Log levels defined in $KROOT/ktl/keyword/ktl/apps/util/errlog.h"
echo
echo " Level     Mask      What uses it"
echo "-------   ------    --------------------------------------------------"
echo "DEBUG1         1    Unused"
echo "DEBUG2         2    rpc_if_functions"
echo "DEBUG3         4    portio open and close; timerThread"
echo "DEBUG4         8    kwAccessors"
echo "DEBUG5        16    rpcKey_serverFuncs function entry; portio config"
echo "DEBUG6        32    rcKey_serverFuncs misc entry, broadcast with kw"
echo "DEBUG7        64    rpcKey_serverFuncs broadcast func entry, clnt call time"
echo "DEBUG8       128    portio bytes on I/O, terminal config"
echo "USER1        256    basic server information"
echo "USER2        512    high level function entry/exit"
echo "USER3       1024    low level (hardware) function entry/exit"
echo "USER4       2048    debug information"
echo "TRIGGER1    4096"
echo "TRIGGER2    8092"
echo "TRIGGER3   16384"
echo "TRIGGER4   32768"
